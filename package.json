{
  "name": "my-project",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "backend": "cd backend && npm install && npm start",
    "frontend": "cd frontend && npm install && npm start",
    "dev": "concurrently --kill-others-on-fail \"npm run backend\" \"npm run frontend\"",
    "start": "cd backend && npm install && npm start",
    "build": "cd frontend && npm run build",
    "lint": "eslint \"backend/**/*.js\" \"frontend/src/**/*.{js,jsx}\"",
    "lint:fix": "eslint --fix \"backend/**/*.js\" \"frontend/src/**/*.{js,jsx}\""
  },
  "dependencies": {
    "axios": "^1.3.5",
    "cors": "2.8.5",
    "cross-env": "7.0.3",
    "express": "4.17.1",
    "fs": "^0.0.1-security",
    "joi": "^17.7.0",
    "morgan": "1.10.0",
    "nanoid": "^4.0.2",
    "prop-types": "^15.8.1",
    "react": "^18.0.0",
    "react-dom": "^18.1.0",
    "react-icons": "^4.8.0",
    "react-loader-spinner": "^5.3.4",
    "react-router-dom": "^6.10.0",
    "react-scripts": "5.0.1",
    "sass": "^1.62.0",
    "sass-convert": "^0.5.2",
    "serve": "^14.2.0",
    "util": "^0.12.5",
    "web-vitals": "^2.1.3"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "eslint": "7.19.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "5.2.0",
    "nodemon": "2.0.15",
    "prettier": "^2.4.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "standard",
      "plugin:node/recommended"
    ],
    "plugins": [
      "import",
      "node",
      "promise"
    ],
    "rules": {
      "import/no-anonymous-default-export": "error",
      "node/no-unsupported-features/es-syntax": "off"
    }
  }
}
